<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.GymGuide" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
         <item name="colorPrimary">@color/yellow_menu</item>
    </style>

    <style name="Theme.GymGuide" parent="Base.Theme.GymGuide" />
    <style name="roundedImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">8dp</item>
    </style>

    <style name="fab_rounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="fab_diamond">
        <item name="cornerFamily">cut</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="fab_chat">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
        <item name="android:bottomRightRadius">0dp</item>
    </style>


    <style name="circularImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

</resources>